<br><br>
<div class="jumbotron">
  <h1>Blocitoff To-Do List</h1>
</div>

<%= render partial: 'form', locals: { item: @item } %>

<div class="col-md-12 item-list">
  <% @items.each do |item| %>
    <div class="media">
      <div class="media-body">
        <h4 class="media-heading">
          <%= item.name %>
        </h4>
      </div>
    </div>
  <% end %>
</div>
<br><br>